<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--导航过程页 圆型进度条 -->
    <declare-styleable name="CircleProgressImageView">
        <attr name="max" format="integer"/>				          <!-- 进度最大值 -->
        <attr name="fill" format="boolean"/>			          <!-- 是否填充圆形区域 -->
        <attr name="paint_width" format="reference|dimension"/>	   <!-- 画笔宽度，填充模式下无效，会被重置为0 -->
        <attr name="paint_color" format="reference|color"/>		   <!-- 画笔颜色 -->
        <attr name="inside_interval" format="reference|dimension"/> <!-- 圆形区域向里缩进的距离 -->
        <attr name="text_color" format="reference|color"/> <!-- 字体颜色，默认黑色 -->
        <attr name="progress_paint_width" format="reference|dimension"/> <!-- 进度条宽度 -->
        <attr name="sub_progress_paint_color" format="reference|color" /> <!-- 第二进度条颜色 -->
        <attr name="circle_background_color" format="reference|color" /> <!-- 圆背景颜色 -->
        <attr name="circle_fill" format="boolean" /> <!-- 无背景图片时，背景是否填充 -->
        <attr name="main_progress_cap_round" format="boolean" /> <!-- 主进度条的CAP是否为round，设置为false或者不设置，则采用默认的CAP类型 -->
    </declare-styleable>

    <!-- 通用Title Bar -->
    <declare-styleable name="CommonTitleBar">
        <attr name="backGroundImage" format="integer" localization="suggested" />
        <attr name="rightContentImage" format="integer" localization="suggested" />
        <attr name="rightContentText" format="integer" localization="suggested" />
        <attr name="leftContentText" format="string" localization="suggested" />
        <attr name="leftContentImage" format="integer" localization="suggested" />
        <attr name="middleContentText" format="string" localization="suggested" />
        <attr name="rightContentVisible" format="boolean" localization="suggested" />
    </declare-styleable>

    <!-- 免责声明行 add by nichenjian -->
    <declare-styleable name="DisclaimerLineView">
        <attr name="nsdk_content" format="string"></attr>
    </declare-styleable>

    <!-- 屏幕方向  -->
    <declare-styleable name="LayoutDirectionView">
        <attr name="LayoutDirection" format="enum">
            <enum name="undefined" value="0" />
            <enum name="portrait" value="1" />
            <enum name="landscape" value="2" />
        </attr>
    </declare-styleable>

    <!--ugc 上报按钮 -->
    <declare-styleable name="UgcReportButton">
        <attr name="iv_width" format="dimension"/>
        <attr name="iv_height" format="dimension"/>
        <attr name="iv_text_padding" format="dimension"/>
        <attr name="is_bold_report" format="boolean"/>
        <attr name="iv_text_size" format="dimension|reference"/>
    </declare-styleable>

    <!--雷达页、驾车页、导航中 ugc 上报面板-->
    <declare-styleable name="UgcReportPanelLayout">
        <attr name="panel_margin_left" format="dimension"/>
        <attr name="panel_margin_right" format="dimension"/>
    </declare-styleable>

    <!--收藏按钮-->
    <declare-styleable name="LikeButton">
        <attr name="icon_type" format="string" />
        <attr name="icon_size" format="dimension|reference"/>
        <attr name="like_drawable" format="reference" />
        <attr name="unlike_drawable" format="reference" />
        <attr name="dots_primary_color" format="color|reference" />
        <attr name="dots_secondary_color" format="color|reference" />
        <attr name="circle_start_color" format="color|reference" />
        <attr name="circle_end_color" format="color|reference" />
        <attr name="liked" format="boolean"/>
        <attr name="is_enabled" format="boolean"/>
        <attr name="anim_scale_factor" format="float"/>
    </declare-styleable>

    <declare-styleable name="RouteResultButton">
        <attr name="icon_src_unusable" format="reference"/>
        <attr name="icon_src_normal" format="reference"/>
        <attr name="icon_src_selected" format="reference"/>
        <attr name="icon_width" format="dimension|reference"/>
        <attr name="icon_height" format="dimension|reference"/>
        <attr name="title_text" format="string|reference"/>
        <attr name="title_width" format="dimension|reference"/>
        <attr name="title_height" format="dimension|reference"/>
        <attr name="title_text_size" format="dimension|reference"/>
        <attr name="title_text_color_unusable" format="color|reference"/>
        <attr name="title_text_color_normal" format="color|reference"/>
        <attr name="title_text_color_selected" format="color|reference"/>
        <attr name="title_margin_icon" format="dimension|reference"/>
        <attr name="label_text" format="string|reference"/>
        <attr name="label_text_color" format="integer|reference"/>
        <attr name="label_size" format="dimension|reference"/>
        <attr name="label_background" format="reference"/>
    </declare-styleable>

    <!-- 区间测速  -->
    <declare-styleable name="IntervalSpeedView">
        <attr name="maxSize" format="dimension|reference"></attr>
        <attr name="minSize" format="dimension|reference"></attr>
        <attr name="page" format="enum">
            <enum name="lightNavi" value="1"/>
            <enum name="proNavi" value="2"/>
        </attr>
    </declare-styleable>
</resources>