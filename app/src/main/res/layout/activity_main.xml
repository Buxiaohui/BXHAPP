<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_test"
            style="@style/common_title_mid_txt_style"
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_marginLeft="132px"
            android:layout_marginRight="176px"
            android:background="@color/colorAccent"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:singleLine="true"
            android:text="北京市朝阳区S12(机场高速) > 背景首都国际机场"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/tv_test1"
            style="@style/common_title_mid_txt_style"
            android:layout_marginLeft="132px"
            android:layout_marginRight="176px"
            android:background="#00ff00"
            android:layout_width="match_parent"
            android:gravity="center"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:singleLine="true"
            android:text="北京市朝阳区S12(机场高速) > 背景首都国际机场"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:id="@+id/layout"
            android:layout_width="wrap_content"
            android:background="@android:color/transparent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:visibility="gone"
                android:text="测试文字" />

            <TextView
                android:id="@+id/tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="测试文字"
                android:visibility="gone"
                android:layout_below="@id/tv" />
        </RelativeLayout>

        <LinearLayout
            android:visibility="gone"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="50dp"
            android:divider="@drawable/nsdk_route_car_end_recom_divider"
            android:showDividers="middle"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv01"
                style="@style/item"
                android:layout_height="wrap_content"
                tools:text="(可以停车)\n哈" />

            <TextView
                android:id="@+id/tv02"
                style="@style/item"
                tools:text="item02" />

            <TextView
                android:id="@+id/tv03"
                style="@style/item"
                tools:text="item03" />

        </LinearLayout>

        <TextView
            android:id="@+id/test_end_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:id="@+id/ll"
            android:visibility="gone"
            android:background="@android:color/holo_green_light"
            android:layout_width="match_parent"
            android:layout_height="100dp">

            <include
                android:id="@+id/zhuzi"
                layout="@layout/nsdk_layout_rr_eta_date_time_item_eta_date_time_item"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="10dp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@color/colorAccent"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="10dp"></View>
        </RelativeLayout>

        <com.example.buxiaohui.bxhapp.RouteCarNearbySearchPopup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:id="@+id/container_pop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <LinearLayout
            android:id="@+id/container_pop_l"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <ImageView
            android:id="@+id/container_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <FrameLayout
            android:id="@+id/parent_container"
            android:layout_width="300dp"
            android:background="@color/colorAccent"
            android:layout_height="300dp"></FrameLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>